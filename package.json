{
  "name": "parse-picks",
  "version": "1.0.0",
  "description": "Parses webpage from 4nfl picks",
  "main": "index.js",
  "repository": {
    "type": "Github",
    "url": "git@github.com:bduff9/parse-picks.git"
  },
  "scripts": {
    "lint": "eslint --fix \"**/*.mjs\"",
    "start": "NODE_MODULES=\"\" node --experimental-modules index.mjs",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "coverage": "jest --collectCoverageFrom=src/**.mjs --coverage src",
    "watch": "jest --watchAll"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "cli-spinner": "0.2.10",
    "request": "2.88.2",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "ajv": "6.10.2",
    "babel-core": "6.26.3",
    "babel-jest": "24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "coveralls": "3.0.6",
    "eslint": "6.3.0",
    "jest": "24.9.0",
    "jest-mock-console": "1.0.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.?(m)js?(x)",
      "**/?(*.)(spec|test).?(m)js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "node",
      "mjs"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "spec": true
            }
          ]
        ]
      }
    }
  }
}
